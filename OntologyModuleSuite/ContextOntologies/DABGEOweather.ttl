WeatherOntology:Hail
      a       owl:NamedIndividual , WeatherOntology:WeatherCondition .

WeatherOntology:hasCloudCoverValue
      a       owl:DatatypeProperty , owl:FunctionalProperty ;
      rdfs:comment "The value of a cloud cover weather phenomenon." ;
      rdfs:domain WeatherOntology:CloudCover ;
      rdfs:label "hasCloudCoverValue"@en ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:onDatatype xsd:nonNegativeInteger ;
                owl:withRestrictions
                        ([ xsd:maxExclusive 10
                          ])
              ] .

WeatherOntology:ExtremeFrost
      a       owl:Class ;
      rdfs:label "ExtremeFrost"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Temperature [ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:maxExclusive "-25.0"^^xsd:float
                                                ])
                                    ] ;
                            owl:onProperty <http://www.purl.org/oema/enaeq/hasValue>
                          ])
              ] .

WeatherOntology:belongsToWeatherState
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "Relation between weather states and the phenomena that are observed or forecasted (inverse of 'hasWeatherPhenomenon' property)." ;
      rdfs:label "hasWeatherPhenomenon" ;
      owl:inverseOf WeatherOntology:hasWeatherPhenomenon .

WeatherOntology:SevereWeatherState
      a       owl:Class ;
      rdfs:label "SevereWeatherState"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (WeatherOntology:WeatherState [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Class ;
                                        owl:intersectionOf ([ a       owl:Restriction ;
                                                    owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                                    owl:someValuesFrom WeatherOntology:HeavyPrecipitation
                                                  ] [ a       owl:Restriction ;
                                                    owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                                    owl:someValuesFrom WeatherOntology:StrongWind
                                                  ])
                                      ] [ a       owl:Restriction ;
                                        owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                        owl:someValuesFrom WeatherOntology:ExtremeFrost
                                      ] [ a       owl:Restriction ;
                                        owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                        owl:someValuesFrom WeatherOntology:ExtremeHeat
                                      ] [ a       owl:Restriction ;
                                        owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                        owl:someValuesFrom WeatherOntology:HeavyPrecipitation
                                      ] [ a       owl:Restriction ;
                                        owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                        owl:someValuesFrom WeatherOntology:Storm
                                      ])
                          ])
              ] .

externalfactors:Temperature
      a       owl:Class ;
      rdfs:comment "This class represents exterior temperature."@en ;
      rdfs:label "Temperature"@en ;
      rdfs:subClassOf WeatherOntology:WeatherPhenomenon ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty units:unitOfMeasure ;
                owl:someValuesFrom units:TemperatureUnit
              ] .

WeatherOntology:ModerateGale
      a       owl:Class ;
      rdfs:label "ModerateGale" ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Wind [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive "13.8"^^xsd:float
                                                ])
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
