WeatherOntology:WholeGale
      a       owl:Class ;
      rdfs:label "WholeGale" ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Wind [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive "24.4"^^xsd:float
                                                ])
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:maxInclusive "28.4"^^xsd:float
                                                ])
                                    ]
                          ])
              ] .

WeatherOntology:MostlyCloudy
      a       owl:Class ;
      rdfs:label "MostlyCloudy"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (WeatherOntology:CloudCover [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasCloudCoverValue ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:nonNegativeInteger ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive 5
                                                ])
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasCloudCoverValue ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:nonNegativeInteger ;
                                      owl:withRestrictions
                                              ([ xsd:maxExclusive 8
                                                ])
                                    ]
                          ])
              ] .

WeatherOntology:PartlyCloud
      a       owl:NamedIndividual , WeatherOntology:WeatherCondition .

externalfactors:RainfallTotal
      a       owl:Class ;
      rdfs:label "RainfallTotal"@en ;
      rdfs:subClassOf WeatherOntology:Precipitation .

WeatherOntology:Hurricane
      a       owl:Class ;
      rdfs:label "Hurricane" ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Wind [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive "32.6"^^xsd:float
                                                ])
                                    ]
                          ])
              ] .

WeatherOntology:FairWeatherState
      a       owl:Class ;
      rdfs:label "FairWeatherState"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (WeatherOntology:WeatherState [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                            owl:someValuesFrom WeatherOntology:NoPrecipitation
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                            owl:someValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (WeatherOntology:LightWind WeatherOntology:NoWind)
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                            owl:someValuesFrom
                                    [ a       owl:Class ;
                                      owl:unionOf (WeatherOntology:NoCloudCover WeatherOntology:PartlyCloudy)
                                    ]
                          ])
              ] .

WeatherOntology:hasIntensity
      a       owl:DatatypeProperty ;
      rdfs:comment "Intensity value of precipitation." ;
      rdfs:domain WeatherOntology:Precipitation ;
      rdfs:label "hasIntensity"@en ;
      rdfs:range xsd:float .

