WeatherOntology:hasDirection
      a       owl:DatatypeProperty , owl:FunctionalProperty ;
      rdfs:comment "The direction of the wind." ;
      rdfs:domain externalfactors:Wind ;
      rdfs:label "hasDirection"@en ;
      rdfs:range
              [ a       rdfs:Datatype ;
                owl:onDatatype xsd:float ;
                owl:withRestrictions
                        ([ xsd:maxExclusive "360.0"^^xsd:float
                          ])
              ] .

WeatherOntology:LightBreeze
      a       owl:Class ;
      rdfs:label "LightBreeze" ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Wind [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive "1.5"^^xsd:float
                                                ])
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:maxInclusive "3.4"^^xsd:float
                                                ])
                                    ]
                          ])
              ] .

[]    a       owl:AllDisjointClasses ;
      owl:members (WeatherOntology:DryHumidity WeatherOntology:MoistHumidity WeatherOntology:OptimumHumidity) .

WeatherOntology:ModeratePrecipitation
      a       owl:Class ;
      rdfs:label "ModeratePrecipitation"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (WeatherOntology:Precipitation [ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive "1.0"^^xsd:float
                                                ])
                                    ] ;
                            owl:onProperty WeatherOntology:hasIntensity
                          ] [ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:maxInclusive "4.0"^^xsd:float
                                                ])
                                    ] ;
                            owl:onProperty WeatherOntology:hasIntensity
                          ])
              ] .

WeatherOntology:ExtremeHeat
      a       owl:Class ;
      rdfs:label "ExtremeHeat"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Temperature [ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minInclusive "37.0"^^xsd:float
                                                ])
                                    ] ;
                            owl:onProperty <http://www.purl.org/oema/enaeq/hasValue>
                          ])
              ] .

WeatherOntology:WeatherCondition
      a       owl:Class ;
      rdfs:comment "One-word description of the observed weather condition" ;
      rdfs:label "WeatherCondition" ;
      rdfs:subClassOf externalfactors:ExternalFactor ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:oneOf (WeatherOntology:Cloud WeatherOntology:Fog WeatherOntology:Hail WeatherOntology:LightCloud WeatherOntology:LightRainSun WeatherOntology:PartlyCloud WeatherOntology:Rain WeatherOntology:Sleet WeatherOntology:Snow WeatherOntology:Sun WeatherOntology:Thunder)
              ] .

externalfactors:ExternalFactor
      a       owl:Class .

[]    a       owl:AllDisjointClasses ;
      owl:members (WeatherOntology:HighSolarIrradiance WeatherOntology:LowSolarIrradiance WeatherOntology:ModerateSolarIrradiance WeatherOntology:NeglectibleSolarIrradiance) .

WeatherOntology:Cloud
      a       owl:NamedIndividual , WeatherOntology:WeatherCondition .

<http://www.purl.org/dabgeo/common-domain/weather>
      a       owl:Ontology ;
      rdfs:comment """The 'Weather' ontology module represents different weather phenomenon such as humidity or temperature.
This ontology module reuses the knowledge of the 'External Factors' ontology module.

Information about changes compared to version 1.1
*********************************************************
We have changed the names of ontology layers and documented missing classes."""@en ;
      ns:license "https://creativecommons.org/licenses/by-nc/4.0/" ;
      dc:creator "Javier Cuenca <mailto:jcuenca@mondragon.edu>"@en ;
      dc:title "Weather ontology"@en ;
      terms:issued "2018-02-09"@en ;
      terms:modified "2019-02-22"@en ;
      owl:imports <http://www.purl.org/oema/units> , <http://www.w3.org/2006/time#2016> , <http://www.purl.org/dabgeo/common-domain/externalfactors> ;
      owl:versionInfo "2.0"@en .

WeatherOntology:hasWeatherCondition
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:comment "relation of weather states and a one-word description for the observed weather situation"@en ;
      rdfs:domain WeatherOntology:WeatherState ;
      rdfs:label "hasWeatherCondition"@en ;
      rdfs:range WeatherOntology:WeatherCondition .

WeatherOntology:RainyWeatherState
      a       owl:Class ;
      rdfs:label "RainyWeatherState"@en ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:hasValue WeatherOntology:Rain ;
                owl:onProperty WeatherOntology:hasWeatherCondition
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (WeatherOntology:WeatherState [ a       owl:Class ;
                            owl:complementOf
                                    [ a       owl:Restriction ;
                                      owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                                      owl:someValuesFrom WeatherOntology:NoPrecipitation
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasWeatherPhenomenon ;
                            owl:someValuesFrom WeatherOntology:AboveZeroTemperature
                          ])
              ] .

<http://www.purl.org/oema/enaeq/hasValue>
      a       owl:DatatypeProperty , owl:FunctionalProperty ;
      rdfs:comment "The value of a parameter." ;
      rdfs:label "hasValue" .

WeatherOntology:Snow
      a       owl:NamedIndividual , WeatherOntology:WeatherCondition .

units:HumidityUnit
      a       owl:Class .

WeatherOntology:StrongBreeze
      a       owl:Class ;
      rdfs:label "StrongBreeze" ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Wind [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:minExclusive "10.7"^^xsd:float
                                                ])
                                    ]
                          ] [ a       owl:Restriction ;
                            owl:onProperty WeatherOntology:hasSpeed ;
                            owl:someValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:maxInclusive "13.8"^^xsd:float
                                                ])
                                    ]
                          ])
              ] .

WeatherOntology:AirPollution
      a       owl:Class ;
      rdfs:comment "This class represents air pollution represented as European Air Quality Index value. The index value is based on 3 pollutants of major concern in Europe: PM10, NO2, O3 and will be able to take into account to 3 additional pollutants (CO, PM2.5 and SO2) where data are also available."@en ;
      rdfs:label "AirPollution"@en ;
      rdfs:subClassOf WeatherOntology:WeatherPhenomenon ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty <http://www.purl.org/oema/enaeq/hasValue> ;
                owl:someValuesFrom xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty units:unitOfMeasure ;
                owl:someValuesFrom units:UnitOfMeasure
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (WeatherOntology:HighAirPollution WeatherOntology:LowAirPollution WeatherOntology:MediumAirPollution WeatherOntology:VeryHighAirPollution WeatherOntology:VeryLowAirPollution)
              ] .

WeatherOntology:Rain
      a       owl:NamedIndividual , WeatherOntology:WeatherCondition .

WeatherOntology:DryHumidity
      a       owl:Class ;
      rdfs:label "DryHumidity"@en ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (externalfactors:Humidity [ a       owl:Restriction ;
                            owl:allValuesFrom
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:float ;
                                      owl:withRestrictions
                                              ([ xsd:maxInclusive "25.0"^^xsd:float
                                                ])
                                    ] ;
                            owl:onProperty <http://www.purl.org/oema/enaeq/hasValue>
                          ])
              ] .

WeatherOntology:CloudCover
      a       owl:Class ;
      rdfs:comment "This class represents the clound cover in the unit \"Okta\""@en ;
      rdfs:label "CloudCover"@en ;
      rdfs:subClassOf WeatherOntology:WeatherPhenomenon ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:onDataRange
                                    [ a       rdfs:Datatype ;
                                      owl:onDatatype xsd:nonNegativeInteger ;
                                      owl:withRestrictions
                                              ([ xsd:maxExclusive 10
                                                ])
                                    ] ;
                            owl:onProperty WeatherOntology:hasCloudCoverValue ;
                            owl:qualifiedCardinality
                                    "1"^^xsd:nonNegativeInteger
                          ] [ a       owl:Restriction ;
                            owl:maxQualifiedCardinality
                                    "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:nonNegativeInteger ;
                            owl:onProperty WeatherOntology:hasCloudAltitude
                          ])
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:unionOf (WeatherOntology:MostlyCloudy WeatherOntology:NoCloudCover WeatherOntology:Overcast WeatherOntology:PartlyCloudy WeatherOntology:UnknownCloudCover)
              ] .

externalfactors:SolarDeclination
      a       owl:Class ;
      rdfs:comment "This class represents the position of the sun in the sky. The position of the sun in the sky is a function of both the time and the geographic location of observation on Earth's surface."@en ;
      rdfs:label "SolarDeclination"@en ;
      rdfs:subClassOf WeatherOntology:WeatherPhenomenon .

WeatherOntology:GentleBreeze
