<?xml version="1.0"?>
<rdf:RDF xmlns="http://cpsagila.cs.uni-kl.de/GENIALOnt#"
     xml:base="http://cpsagila.cs.uni-kl.de/GENIALOnt"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:CarModel="http://cpsagila.cs.uni-kl.de/GENIALOnt/CarModel/"
     xmlns:hardware="http://cpsagila.cs.uni-kl.de/GENIALOnt/hardware/"
     xmlns:CarModel1="http://w3id.org/gbo/CarModel/">
    <owl:Ontology rdf:about="http://w3id.org/gbo">
        <owl:versionIRI rdf:resource="http://w3id.org/gbo/0.2.1"/>
        <owl:imports rdf:resource="http://purl.obolibrary.org/obo/bfo/2.0/bfo.owl"/>
        <dc:description>Basic Ontology with general vocabulary for serving as a ground to execute basic tasks of the tool AGILA and share common upper level for all classes in GENIAL!. It supports the modelling of microlectronic components like sensors, acutators, processors and software along the automotive value chain.</dc:description>
        <dc:title>GENIAL! Basic Ontology</dc:title>
        <terms:contributor>Christoph Grimm</terms:contributor>
        <terms:contributor>Peter Neumann</terms:contributor>
        <terms:created>2021-01-26</terms:created>
        <terms:creator>Frank Wawrzik</terms:creator>
        <terms:title>GENIAL! Basic Ontology</terms:title>
        <rdfs:label>GENIAL! Basic Ontology</rdfs:label>
        <owl:versionInfo>0.2.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://w3id.org/gbo/CarModel/atTime -->

    <owl:AnnotationProperty rdf:about="http://w3id.org/gbo/CarModel/atTime">
        <obo:IAO_0000112>e.g. law_for_banning_combustion_engines bans combustion engine atTime 2030-10-26T21:32:52 (xsd:DateTime)</obo:IAO_0000112>
        <rdfs:comment>used to annotate triples</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:AnnotationProperty>
    


    <!-- http://w3id.org/gbo/CarModel/hasAPPELFormula -->

    <owl:AnnotationProperty rdf:about="http://w3id.org/gbo/CarModel/hasAPPELFormula"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/gbo#comprised_of -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#comprised_of">
        <owl:equivalentProperty rdf:resource="http://w3id.org/gbo#has_part_directly"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#depends_on -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#depends_on">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/gbo#property"/>
        <rdfs:range rdf:resource="http://w3id.org/gbo#dependency"/>
        <obo:IAO_0000115>Links properties with dependencies.</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#executes -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#executes">
        <owl:inverseOf rdf:resource="http://w3id.org/gbo#is_executed_by"/>
        <rdfs:domain rdf:resource="http://w3id.org/gbo/CarModel/processing_element"/>
        <rdfs:range rdf:resource="http://w3id.org/gbo#software_element"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#has_part -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#has_part">
        <owl:inverseOf rdf:resource="http://w3id.org/gbo#part_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <obo:IAO_0000115>models hierarchical de-composition/integration</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#has_part_directly -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#has_part_directly">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/gbo#has_part"/>
        <owl:inverseOf rdf:resource="http://w3id.org/gbo#part_of_directly"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#has_property -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#has_property">
        <owl:inverseOf rdf:resource="http://w3id.org/gbo#property_of"/>
        <rdfs:range rdf:resource="http://w3id.org/gbo#property"/>
        <obo:IAO_0000115>links components to their properties</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#implements -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#implements">
        <owl:inverseOf rdf:resource="http://w3id.org/gbo#is_implemented_by"/>
        <rdfs:domain rdf:resource="http://w3id.org/gbo#element"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#is_executed_by -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#is_executed_by">
        <rdfs:domain rdf:resource="http://w3id.org/gbo#software_element"/>
        <rdfs:range rdf:resource="http://w3id.org/gbo/CarModel/processing_element"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#is_implemented_by -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#is_implemented_by">
        <rdfs:domain rdf:resource="http://w3id.org/gbo#function"/>
        <rdfs:range rdf:resource="http://w3id.org/gbo#element"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#part_of -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#part_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <obo:IAO_0000115>models hierarchical de-composition/integration</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#part_of_directly -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#part_of_directly">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/gbo#part_of"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/gbo#property_of -->

    <owl:ObjectProperty rdf:about="http://w3id.org/gbo#property_of">
        <obo:IAO_0000115>links properties back to their components</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit -->

    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/measure"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/unit"/>
        <obo:IAO_0000115>Links measure with their unit</obo:IAO_0000115>
        <rdfs:label>has unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue -->

    <owl:ObjectProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue">
        <rdfs:domain rdf:resource="http://w3id.org/gbo#property"/>
        <rdfs:range rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/measure"/>
        <obo:IAO_0000115>Links properties or quantities to their measure (concrete value)</obo:IAO_0000115>
        <rdfs:label>has value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/gbo/hasLowerBoundNumericalValue -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/gbo/hasLowerBoundNumericalValue"/>
    


    <!-- http://w3id.org/gbo/hasUpperBoundNumericalValue -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/gbo/hasUpperBoundNumericalValue"/>
    


    <!-- http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue -->

    <owl:DatatypeProperty rdf:about="http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue">
        <rdfs:domain rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/measure"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/gbo#component -->

    <owl:Class rdf:about="http://w3id.org/gbo#component">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/gbo#non_system_level_element"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://w3id.org/gbo#comprised_of"/>
                                        <owl:someValuesFrom rdf:resource="http://w3id.org/gbo#hardware_part"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://w3id.org/gbo#comprised_of"/>
                                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                                        <owl:onClass rdf:resource="http://w3id.org/gbo#hardware_part"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://w3id.org/gbo#comprised_of"/>
                                        <owl:someValuesFrom rdf:resource="http://w3id.org/gbo#software_component"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://w3id.org/gbo#comprised_of"/>
                                        <owl:someValuesFrom rdf:resource="http://w3id.org/gbo#software_unit"/>
                                    </owl:Restriction>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#part_of_directly"/>
                        <owl:someValuesFrom rdf:resource="http://w3id.org/gbo#system"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <obo:IAO_0000112>A microcontroller</obo:IAO_0000112>
        <obo:IAO_0000115>according to ISO26262: non-system level element (3.41) that is logically or technically separable and is comprised of more than one hardware part (3.71) or one or more software units (3.159). 

A component is a part of a system (3.163).</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#context -->

    <owl:Class rdf:about="http://w3id.org/gbo#context">
        <obo:IAO_0000115>The circumstances that form the setting for an event, statement or idea and in terms of which it can be fully understood, e.g. Market, legal  frame conditions</obo:IAO_0000115>
        <rdfs:label>context</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#dependency -->

    <owl:Class rdf:about="http://w3id.org/gbo#dependency">
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#property"/>
        <obo:IAO_0000115>A gbo:property that is a boolean or arithmetic function / equation on properties. Used to be calculated with GENIAL constraint propagation. As string in LaTex syntax</obo:IAO_0000115>
        <rdfs:label>dependency</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#element -->

    <owl:Class rdf:about="http://w3id.org/gbo#element">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000002"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/gbo#has_property"/>
                <owl:someValuesFrom rdf:resource="http://w3id.org/gbo/operating_time"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000020"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000141"/>
                    <rdf:Description rdf:about="http://w3id.org/gbo#context"/>
                </owl:unionOf>
            </owl:Class>
        </owl:disjointWith>
        <obo:IAO_0000115>Continuant that is (according to ISO 26262) a system (3.163), components (3.21) (hardware or software), hardware parts (3.71), or software units (3.159)</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#engineered_object -->

    <owl:Class rdf:about="http://w3id.org/gbo#engineered_object">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000030"/>
        <obo:IAO_0000115>An object created, designed or engineered by humans</obo:IAO_0000115>
        <rdfs:label>engineered object</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#function -->

    <owl:Class rdf:about="http://w3id.org/gbo#function">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000034"/>
        <obo:IAO_0000115>A bfo:function that an element (e.g. system, component, hardware or software) implements</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#hardware_component -->

    <owl:Class rdf:about="http://w3id.org/gbo#hardware_component">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_element"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#comprised_of"/>
                        <owl:allValuesFrom rdf:resource="http://w3id.org/gbo#hardware_part"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#comprised_of"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://w3id.org/gbo#hardware_part"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#component"/>
        <owl:disjointWith rdf:resource="http://w3id.org/gbo#software_component"/>
        <obo:IAO_0000112>actuator</obo:IAO_0000112>
        <obo:IAO_0000112>multi core</obo:IAO_0000112>
        <obo:IAO_0000112>sensor</obo:IAO_0000112>
        <obo:IAO_0000115>No ISO definition. Formal Definition derived from other definitions.</obo:IAO_0000115>
        <rdfs:label>hardware component</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#hardware_element -->

    <owl:Class rdf:about="http://w3id.org/gbo#hardware_element">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_component"/>
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_elementary_subpart"/>
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_part"/>
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_subpart"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#engineered_object"/>
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#non_system_level_element"/>
        <owl:disjointWith rdf:resource="http://w3id.org/gbo#software_element"/>
        <rdfs:comment>From definition of element:
Note 1 to entry: When “software element” or “hardware element” is used, this phrase denotes an element of software only or an element of hardware only, respectively.</rdfs:comment>
        <rdfs:label>hardware</rdfs:label>
        <rdfs:label>hardware element</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#hardware_elementary_subpart -->

    <owl:Class rdf:about="http://w3id.org/gbo#hardware_elementary_subpart">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_element"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#part_of_directly"/>
                        <owl:allValuesFrom rdf:resource="http://w3id.org/gbo#hardware_subpart"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://w3id.org/gbo/actuating"/>
        <obo:IAO_0000112>A flip-flop of the ALU with its logic cone, a register.</obo:IAO_0000112>
        <obo:IAO_0000115>smallest portion of a hardware subpart (3.73) considered in safety (3.132) analysis</obo:IAO_0000115>
        <rdfs:label>hardware elementary subpart</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#hardware_part -->

    <owl:Class rdf:about="http://w3id.org/gbo#hardware_part">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_element"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#part_of_directly"/>
                        <owl:allValuesFrom rdf:resource="http://w3id.org/gbo#component"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <obo:IAO_0000112>The CPU of a microcontroller, a resistor, flash array of a microcontroller.</obo:IAO_0000112>
        <obo:IAO_0000115>a piece of hardware that is (according to ISO 26262) a portion of a hardware component (3.21) at the first level of hierarchical decomposition</obo:IAO_0000115>
        <rdfs:label>hardware part</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#hardware_subpart -->

    <owl:Class rdf:about="http://w3id.org/gbo#hardware_subpart">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_element"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#has_part_directly"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_elementary_subpart"/>
                                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_subpart"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/gbo#part_of_directly"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_part"/>
                                    <rdf:Description rdf:about="http://w3id.org/gbo#hardware_subpart"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <obo:IAO_0000112>ALU of a CPU of a microcontroller</obo:IAO_0000112>
        <obo:IAO_0000112>register bank of a CPU</obo:IAO_0000112>
        <obo:IAO_0000115>portion of a hardware part (3.71) that can be logically divided and represents second or greater level of hierarchical decomposition</obo:IAO_0000115>
        <rdfs:label>hardware subpart</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#legal_boundary_condition -->

    <owl:Class rdf:about="http://w3id.org/gbo#legal_boundary_condition">
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#context"/>
        <rdfs:label>legal boundary conditon</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#market -->

    <owl:Class rdf:about="http://w3id.org/gbo#market">
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#context"/>
        <rdfs:label>market</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#material_object -->

    <owl:Class rdf:about="http://w3id.org/gbo#material_object">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000030"/>
        <rdfs:label>material object</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#non_system_level_element -->

    <owl:Class rdf:about="http://w3id.org/gbo#non_system_level_element">
        <rdfs:subClassOf rdf:resource="http://w3id.org/gbo#element"/>
        <owl:disjointWith rdf:resource="http://w3id.org/gbo#system_level_element"/>
        <obo:IAO_0000115>An element that is not on the system-level</obo:IAO_0000115>
        <rdfs:label>non system level element</rdfs:label>
    </owl:Class>
    


    <!-- http://w3id.org/gbo#property -->

    <owl:Class rdf:about="http://w3id.org/gbo#property">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontology-of-units-of-measure.org/resource/om-2/hasValue"/>
